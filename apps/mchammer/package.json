{
  "name": "@topplethenun/mchammer",
  "type": "module",
  "author": "Richard Harrah",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "imports": {
    "#*": "./*"
  },
  "scripts": {
    "build": "nr build:generate-env && nr build:generate-routes && nr build:icons && nr build:remix",
    "build:generate-env": "tsx other/generate-env.ts",
    "build:generate-routes": "routes-gen -d @routes-gen/remix && prettier --write app/routes.d.ts",
    "build:icons": "tsx other/build-icons.ts",
    "build:remix": "remix build --sourcemap",
    "clean": "rimraf build public/build",
    "dev": "nr build:generate-env && nr build:generate-routes && nr build:icons && nr with-env remix dev",
    "gql": "nr gql:introspection && nr gql:generate",
    "gql:generate": "nr with-env graphql-codegen && prettier --write app/wcl/types.ts && prettier --write schema.json && prettier --write app/wcl/gql/schema.graphql",
    "gql:introspection": "nr with-env tsx -r dotenv/config app/wcl/queries/introspection.ts",
    "lint": "eslint app",
    "lint:fix": "eslint --fix app",
    "start": "tsx other/generate-env.ts && nr with-env remix-serve build",
    "test": "vitest --passWithNoTests",
    "typecheck": "tsc",
    "vercel-build": "nr build && sentry-upload-sourcemaps && rimraf build/*.map && rimraf build/**/*.map && rimraf public/build/*.map && rimraf public/build/**/*.map",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@conform-to/react": "0.9.1",
    "@conform-to/zod": "0.9.1",
    "@radix-ui/react-avatar": "1.0.4",
    "@radix-ui/react-dialog": "1.0.5",
    "@radix-ui/react-dropdown-menu": "2.0.6",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-popover": "1.0.7",
    "@radix-ui/react-scroll-area": "1.0.5",
    "@radix-ui/react-select": "2.0.0",
    "@radix-ui/react-slot": "1.0.2",
    "@remix-run/css-bundle": "2.2.0",
    "@remix-run/node": "2.2.0",
    "@remix-run/react": "2.2.0",
    "@remix-run/router": "1.11.0",
    "@remix-run/serve": "2.2.0",
    "@sentry/remix": "7.80.1",
    "@urql/core": "4.1.4",
    "@vercel/analytics": "1.1.1",
    "@vercel/kv": "0.2.4",
    "@vercel/postgres": "0.5.1",
    "@vercel/remix": "2.2.0",
    "class-variance-authority": "0.7.0",
    "clsx": "2.0.0",
    "cmdk": "0.2.0",
    "cookie": "0.5.0",
    "date-fns": "2.30.0",
    "drizzle-orm": "0.28.6",
    "graphql": "16.8.1",
    "graphql-request": "6.1.0",
    "graphql-tag": "2.12.6",
    "isbot": "3.7.0",
    "kleur": "4.1.5",
    "lodash-es": "4.17.21",
    "lucide-react": "0.292.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-wrap-balancer": "1.1.0",
    "tailwind-merge": "2.0.0",
    "tailwindcss-animate": "1.0.7",
    "tiny-invariant": "1.3.1",
    "web-vitals": "3.5.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@antfu/ni": "0.21.9",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-graphql-request": "6.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@graphql-codegen/typescript-urql": "4.0.0",
    "@remix-run/dev": "2.2.0",
    "@remix-run/eslint-config": "2.2.0",
    "@routes-gen/remix": "0.3.7",
    "@sly-cli/sly": "1.4.10",
    "@topplethenun/mchammer-tsconfig": "workspace:*",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/cookie": "0.5.3",
    "@types/fs-extra": "11.0.3",
    "@types/glob": "8.1.0",
    "@types/lodash-es": "4.17.10",
    "@types/node": "20.8.9",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@urql/introspection": "1.0.2",
    "@vitejs/plugin-react": "4.1.0",
    "autoprefixer": "10.4.16",
    "dotenv": "16.3.1",
    "dotenv-cli": "7.3.0",
    "drizzle-kit": "0.19.13",
    "eslint": "8.52.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "execa": "8.0.1",
    "fontpie": "0.3.0",
    "fs-extra": "11.1.1",
    "glob": "10.3.10",
    "jsdom": "22.1.0",
    "node-html-parser": "6.1.11",
    "ofetch": "1.3.3",
    "rimraf": "5.0.5",
    "routes-gen": "0.6.1",
    "tailwindcss": "3.3.5",
    "tsconfig-paths": "4.2.0",
    "tsx": "3.14.0",
    "typescript": "5.2.2",
    "vitest": "0.34.6"
  },
  "sideEffects": false,
  "graphql": {
    "schema": "./schema.json"
  }
}
