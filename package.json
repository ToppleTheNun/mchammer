{
  "name": "mchammer",
  "type": "module",
  "author": "Richard Harrah",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  },
  "imports": {
    "#*": "./*"
  },
  "scripts": {
    "build": "nr build:generate-env && nr build:generate-routes && nr build:icons && nr build:remix",
    "build:generate-env": "tsx other/generate-env.ts",
    "build:generate-routes": "routes-gen -d @routes-gen/remix && prettier --write app/routes.d.ts",
    "build:icons": "tsx other/build-icons.ts",
    "build:remix": "remix build --sourcemap",
    "dev": "nr build:generate-env && nr build:generate-routes && nr build:icons && remix dev",
    "gql": "nr gql:introspection && nr gql:generate",
    "gql:generate": "graphql-codegen && prettier --write app/wcl/types.ts && prettier --write schema.json && prettier --write app/wcl/gql/schema.graphql",
    "gql:introspection": "tsx -r dotenv/config app/wcl/queries/introspection.ts",
    "lint": "eslint app && prettier -c app",
    "lint:fix": "eslint --fix app && prettier --write app",
    "start": "tsx other/generate-env.ts && remix-serve build",
    "typecheck": "tsc",
    "vercel-build": "nr build && sentry-upload-sourcemaps && rimraf build/*.map && rimraf build/**/*.map && rimraf public/build/*.map && rimraf public/build/**/*.map"
  },
  "dependencies": {
    "@conform-to/react": "0.9.0",
    "@conform-to/zod": "0.9.0",
    "@radix-ui/react-avatar": "1.0.4",
    "@radix-ui/react-dialog": "1.0.5",
    "@radix-ui/react-dropdown-menu": "2.0.6",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-popover": "1.0.7",
    "@radix-ui/react-scroll-area": "1.0.5",
    "@radix-ui/react-select": "2.0.0",
    "@radix-ui/react-slot": "1.0.2",
    "@remix-run/css-bundle": "2.1.0",
    "@remix-run/node": "2.1.0",
    "@remix-run/react": "2.1.0",
    "@remix-run/router": "1.10.0",
    "@remix-run/serve": "2.1.0",
    "@sentry/remix": "7.74.1",
    "@urql/core": "4.1.4",
    "@vercel/analytics": "1.1.1",
    "@vercel/kv": "0.2.3",
    "@vercel/postgres": "0.5.0",
    "@vercel/remix": "2.1.0",
    "class-variance-authority": "0.7.0",
    "clsx": "2.0.0",
    "cmdk": "0.2.0",
    "cookie": "0.5.0",
    "date-fns": "2.30.0",
    "drizzle-orm": "0.28.6",
    "graphql": "16.8.1",
    "graphql-request": "6.1.0",
    "graphql-tag": "2.12.6",
    "isbot": "3.7.0",
    "kleur": "4.1.5",
    "lodash-es": "4.17.21",
    "lucide-react": "0.288.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-wrap-balancer": "1.1.0",
    "tailwind-merge": "1.14.0",
    "tailwindcss-animate": "1.0.7",
    "tiny-invariant": "1.3.1",
    "web-vitals": "3.5.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@antfu/ni": "0.21.8",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-graphql-request": "6.0.0",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@graphql-codegen/typescript-urql": "4.0.0",
    "@remix-run/dev": "2.1.0",
    "@remix-run/eslint-config": "2.1.0",
    "@routes-gen/remix": "0.3.6",
    "@sly-cli/sly": "1.4.10",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/cookie": "0.5.3",
    "@types/fs-extra": "11.0.3",
    "@types/glob": "8.1.0",
    "@types/lodash-es": "4.17.10",
    "@types/node": "18.18.6",
    "@types/react": "18.2.31",
    "@types/react-dom": "18.2.14",
    "@urql/introspection": "1.0.2",
    "@vitejs/plugin-react": "4.1.0",
    "autoprefixer": "10.4.16",
    "dotenv": "16.3.1",
    "drizzle-kit": "0.19.13",
    "eslint": "8.52.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "execa": "8.0.1",
    "fontpie": "0.3.0",
    "fs-extra": "11.1.1",
    "glob": "10.3.10",
    "jsdom": "22.1.0",
    "node-html-parser": "6.1.10",
    "ofetch": "1.3.3",
    "prettier": "3.0.3",
    "prettier-plugin-pkg": "0.18.0",
    "prettier-plugin-tailwindcss": "0.5.6",
    "rimraf": "5.0.5",
    "routes-gen": "0.6.1",
    "tailwindcss": "3.3.5",
    "tsconfig-paths": "4.2.0",
    "tsx": "3.14.0",
    "typescript": "5.2.2"
  },
  "sideEffects": false,
  "graphql": {
    "schema": "./schema.json"
  },
  "pnpm": {
    "patchedDependencies": {
      "@sly-cli/sly@1.4.10": "patches/@sly-cli__sly@1.4.10.patch"
    }
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-pkg",
      "prettier-plugin-tailwindcss"
    ]
  }
}
