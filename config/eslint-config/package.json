    "typecheck": "tsc",
    "with-env": "dotenv -e ../../.env --"
  },
  "peerDependencies": {
    "eslint": "^8.54.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "2.1.2",
    "eslint-plugin-react": "7.33.2"
  },
  "devDependencies": {
    "@topplethenun/mchammer-tsconfig": "workspace:*",
    "eslint": "8.54.0",
    "rimraf": "5.0.5",
    "tsup": "8.0.1",
    "typescript": "5.3.2",
    "vitest": "0.34.6"
  }
}
